{
  //预设：Babel插件集合
  "presets": [
    [
      "@babel/preset-env",
      //配置polyfill兼容
      {
        "targets": ["> 1%", "last 1 version"],
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ]
  ],
  "plugins": [
    // 兼容async/await的语法，需要regenratorRuntime插件支持，安装@babel/runtime和@babel/plugin-transform-runtime"
    "@babel/plugin-transform-runtime"
  ]
}
